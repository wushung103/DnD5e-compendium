mily: Arial, Verdana, sans-serif; font-size: 13.3333px;\"><span style=\"font-size: 13.3333px;\">&nbsp;</span></div>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":null},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"requirements":"Druid 1","recharge":{"value":null,"charged":false},"attributes":{"spelldc":10}},"sort":300001,"flags":{"magicitems":{"enabled":false,"equipped":false,"attuned":false,"charges":"0","chargeType":"c1","destroy":false,"destroyFlavorText":"reaches 0 charges: it crumbles into ashes and is destroyed.","rechargeable":false,"recharge":"0","rechargeType":"t1","rechargeUnit":"r1","sorting":"l"},"cf":{"id":"temp_8hyuiuazy5j","path":"職業#/CF_SEP/德魯伊 Druid","color":"#000000"},"dae":{"activeEquipped":false,"alwaysActive":true},"core":{"sourceId":"Item.oOzIYrvN4AXt78mB"},"favtab":{"isFavorite":false}},"img":"https://assets.forge-vtt.com/bazaar/systems/dnd5e/assets/icons/skills/yellow_41.jpg","effects":[{"_id":"4rBn8epw3DFMq76J","flags":{"dae":{"stackable":false,"specialDuration":"None","transfer":true}},"changes":[{"key":"data.traits.languages.custom","value":"德魯伊語","mode":0,"priority":20}],"disabled":false,"duration":{"startTime":1621353832324},"icon":"https://assets.forge-vtt.com/bazaar/systems/dnd5e/assets/icons/skills/yellow_41.jpg","label":"德魯伊語 Druidic","tint":null,"transfer":true}],"folder":null,"permission":{"default":0}},{"_id":"BfaKF2DwJBzUrYsC","name":"半身人靈巧 Halfling Nimbleness","type":"feat","data":{"description":{"value":"<div>&nbsp;</div>\n<div>你可以穿越任何體形比你大的生物所在空間。</div>","chat":"","unidentified":""},"source":"PHB pg. 28",