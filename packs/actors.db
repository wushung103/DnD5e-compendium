 Enemy}</div>","chat":"","unidentified":""},"source":"TCE","activation":{"type":"special","cost":1,"condition":""},"duration":{"value":null,"units":"inst"},"target":{"value":1,"width":null,"units":"any","type":"enemy"},"range":{"value":null,"long":null,"units":"any"},"uses":{"value":2,"max":"3","per":"lr"},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"other","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":null},"damage":{"parts":[],"versatile":""},"formula":"1d6","save":{"ability":"","dc":null,"scaling":"spell"},"requirements":"Ranger 1","recharge":{"value":null,"charged":false},"attributes":{"spelldc":10}},"effects":[{"_id":"7kcGiHDqy6jN9rHx","changes":[{"key":"macro.tokenMagic","mode":0,"value":"outline","priority":"20"}],"disabled":false,"duration":{"startTime":null},"icon":"icons/creatures/invertebrates/fly-wasp-mosquito-green.webp","label":"夙敵 Favored Foe","transfer":false,"flags":{"core":{"statusId":"TRUE"},"dae":{"stackable":"none","specialDuration":[],"transfer":false}},"tint":null,"selectedKey":"macro.tokenMagic"}],"folder":null,"sort":1450000,"permission":{"default":0,"uDAdlkna037LVJd3":3},"flags":{"betterRolls5e":{"critRange":{"type":"String","value":""},"critDamage":{"type":"String","value":""},"quickDesc":{"type":"Boolean","value":true,"altValue":true},"quickAttack":{"type":"Boolean","value":true,"altValue":true},"quickSave":{"type":"Boolean","value":true,"altValue":true},"quickDamage":{"type":"Array","value":[],"altValue":[],"context":[]},"quickProperties":{"type":"Boolean","value":true,"altValue":true},"quickCharges":{"type":"Boolean","value":true,"altValue":true},"quickTemplate":{"type":"Boolean","value":false,"altValue":false},"quickOther":{"type":"Boolean","value":true,"altValue":true,"context":""},"quickFlavor":{"type":"Boolean","value":true,"altValue":true}},"dynamiceffects":{"equipActive":false,"alwaysActive":false,"effects":[]},"magicitems":{"enabled":false,"equipped":false,"attuned":false,"cha